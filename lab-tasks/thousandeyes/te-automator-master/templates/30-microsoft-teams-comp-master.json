{
  "name": "Microsoft Teams End User Experience - Comprehensive",
  "description": "Setup Microsoft Teams testing from your branch offices.",
  "logo_url": "assets/img/brand/ms-teams-logo.png",
  "formVariables": {
    "$formBranchAgents": {
      "name": "Branch Agents",
      "type": "agents"
    },
    "$formUsername": {
      "name": "Teams Username",
      "type": "text"
    },
    "$formPassword": {
      "name": "Teams Password",
      "type": "password"
    },
    "$formTestInterval": {
      "name": "Transit Relay Test Interval",
      "type": "object",
      "options": {
        "120": " Critical - 2 min",
        "300": "   Normal - 5 min",
        "600": "  Relaxed - 10 min"
    },
      "selected": 300
    },
    "$formTransactionTestInterval": {
      "name": "Edge Node Transaction and DNS Trace Test Interval",
      "type": "object",
      "options": {
        "300": " Critical - 5 min",
        "600": "   Normal - 10 min",
        "900": "  Relaxed - 15 min"
      },
      "selected": 600
    }
  },
  "create": {
    "labels": [
      { "name": "DNS Resolution to Edge Node - Teams", "type": "tests" },
      { "name": "Edge Node - Teams", "type": "tests" },
      { "name": "Transport Relay Audio - Teams", "type": "tests" },
      { "name": "Transport Relay Video - Teams", "type": "tests" },
      { "name": "Branch", "type": "agents", "agents": "$formBranchAgents" }
    ],
    "credentials": [
      {
        "name": "Teams_email",
        "value": "$formUsername"
      },
      {
        "name": "Teams_password",
        "value": "$formPassword"
      }
    ],
    "alertRules": [
      {
        "alertType": "BGP",
        "expression": "[(locationId !in {\"8673\"}) && (((prefixLengthIPv4 >= 16) && (prefixLengthIPv4 <= 32)) || ((prefixLengthIPv6 >= 32) && (prefixLengthIPv6 <= 128)))]((reachability < 100%) || (changes > 1))",
        "minimumSources": 1,
        "notifyOnClear": 0,
        "roundsViolatingOutOf": 2,
        "roundsViolatingRequired": 2,
        "roundsViolatingMode": "EXACT",
        "ruleName": "Teams BGP Alert Rule"
      },
      {
        "alertType": "End-to-End (Server)",
        "expression": "((avgLatency >= 200 ms))",
        "minimumSources": 2,
        "notifyOnClear": 1,
        "roundsViolatingOutOf": 3,
        "roundsViolatingRequired": 3,
        "roundsViolatingMode": "EXACT",
        "ruleName": "Teams Delay (Round Trip) Alert Rule"
      },
      {
        "alertType":"DNS Trace",
        "default":0,
        "expression":"((probDetail != \"\"))",
        "minimumSources":1,
        "notifyOnClear":1,
        "roundsViolatingOutOf":3,
        "roundsViolatingRequired":3,
        "roundsViolatingMode":"EXACT",
        "ruleName":"Teams DNS Server Alert Rule"
      },
      {
        "alertType": "Web Transactions",
        "expression": "((duration >= 25000 ms))",
        "minimumSources": 1,
        "notifyOnClear": 1,
        "roundsViolatingOutOf": 3,
        "roundsViolatingRequired": 3,
        "roundsViolatingMode": "EXACT",
        "ruleName": "Teams Edge Node Authentication Transaction Alert Rule"
      },
      {
        "alertType": "End-to-End (Server)",
        "expression": "((loss >= 25%))",
        "minimumSources": 1,
        "notifyOnClear": 1,
        "roundsViolatingOutOf": 3,
        "roundsViolatingRequired": 3,
        "roundsViolatingMode": "EXACT",
        "ruleName": "Teams Packet Loss"
      },
      {
        "alertType": "HTTP Server",
        "expression": "((responseTime >= 300 ms))",
        "minimumSources": 1,
        "notifyOnClear": 1,
        "roundsViolatingOutOf": 3,
        "roundsViolatingRequired": 3,
        "roundsViolatingMode": "EXACT",
        "ruleName": "Teams Web Server Alert Rule"
      }
    ],
    "tests": [
