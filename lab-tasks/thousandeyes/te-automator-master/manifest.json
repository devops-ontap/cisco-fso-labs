{
  "name": "ThousandEyes Automator for Partners",
  "description": "Deploy common ThousandEyes use cases with a press of a button.",
  "version": "2.0.12",
  "homepage_url": "https://cisco.sharepoint.com/sites/ThousandEyes-SE/SitePages/ThousandEyes-Automator.aspx",
  "permissions": [
    "storage",
    "tabs",
    "https://api.thousandeyes.com/",
    "https://app.thousandeyes.com/account/switch/"
  ],
  "icons": {
    "16": "assets/icons/thousandeyes-icon-16.png",
    "32": "assets/icons/thousandeyes-icon-32.png",
    "48": "assets/icons/thousandeyes-icon-48.png",
    "128": "assets/icons/thousandeyes-icon-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/thousandeyes-icon-16.png",
      "32": "assets/icons/thousandeyes-icon-32.png",
      "48": "assets/icons/thousandeyes-icon-48.png",
      "128": "assets/icons/thousandeyes-icon-128.png"
    }
  },
  "manifest_version": 2
}
